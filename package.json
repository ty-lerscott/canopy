{
	"name": "canopy",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"dev:apps": "next dev ./src/apps",
		"dev:tools": "NODE_ENV=development tsx watch src/tools/index.ts",
		"dev:all": "npm run keygen && NODE_ENV=development NODE_OPTIONS=\"--import ./sentry.mjs\" tsx watch src/api/server.ts",
		"build:ts": "tsup",
		"build:apps": "NODE_OPTIONS=\"--import ./sentry.mjs\" next build ./src/apps",
		"build:all": "npm run build:apps && npm run build:ts",
		"start:apps": "NODE_OPTIONS=\"--import ./sentry.mjs\" next start ./src/apps",
		"start:all": "NODE_OPTIONS=\"--import ./sentry.mjs\" node ./dist/api/server.js",
		"lint": "biome lint ./src",
		"format": "biome format ./src --write",
		"check": "biome check ./src",
		"keygen": "tsx src/tools/set-env.ts",
		"tar": "tar --exclude-vcs --exclude=\"src/apps/.next\" -czvf canopy.tar.gz .",
		"zip": "zip -r canopy.zip . -x \"*.git*\" -x \"*.next*\"",
		"clean": "find ./src -type f \\( -name \"*.js\" -o -name \"*.js.map\" \\) -delete && rm -rf dist logs",
		"discord": "node ./src/tools/bots/discord.js"
	},
	"type": "module",
	"author": {
		"note": "LET THIS COME FROM THE API REQUEST OR CONTENTFUL",
		"name": "Tyler Scott"
	},
	"dependencies": {
		"@dotenvx/dotenvx": "1.6.2",
		"@paralleldrive/cuid2": "2.2.2",
		"@sentry/node": "8.17.0",
		"@sentry/profiling-node": "8.17.0",
		"@types/body-parser": "1.19.5",
		"@types/cors": "2.8.17",
		"@types/express": "4.17.21",
		"@types/node": "20",
		"@types/object.omit": "3.0.3",
		"@types/react": "18.3.3",
		"@types/react-dom": "18.3.0",
		"body-parser": "1.20.2",
		"clsx": "2.1.1",
		"cors": "2.8.5",
		"dayjs": "1.11.11",
		"deepmerge": "4.3.1",
		"discord-webhook-node": "1.1.8",
		"express": "4.19.2",
		"jimp": "0.22.12",
		"next": "14.2.4",
		"object.omit": "3.0.0",
		"postcss": "8.4.39",
		"puppeteer": "22.12.1",
		"react": "18.3.1",
		"react-dom": "18.3.1",
		"redoc": "2.1.5",
		"tailwindcss": "3.4.4",
		"tsup": "8.1.0",
		"tsx": "4.16.2",
		"winston": "3.13.0"
	},
	"devDependencies": {
		"@biomejs/biome": "1.8.3",
		"typescript": "5.5.3"
	}
}
