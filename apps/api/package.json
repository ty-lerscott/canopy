{
  "name": "server",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS=\"--import ./src/lib/sentry.mjs\" tsx watch ./src/index.ts",
    "build": "tsup",
    "start": "NODE_OPTIONS=\"--import ./dist/lib/sentry.js\" node ./dist/index.js",
    "lint": "biome lint ./src",
    "format": "biome format ./src --write",
    "check": "biome check ./src",
    "keygen": "tsx src/tools/set-env.ts",
    "tar": "tar --exclude-vcs --exclude=\"src/apps/.next\" -czvf canopy.tar.gz .",
    "zip": "zip -r canopy.zip . -x \"*.git*\" -x \"*.next*\"",
    "clean": "find ./src -type f \\( -name \"*.js\" -o -name \"*.js.map\" \\) -delete && rm -rf dist logs",
    "discord": "node ./src/tools/bots/discord.js"
  },
  "type": "module",
  "author": {
    "name": "Tyler Scott",
    "phone": "607 882 0531",
    "address": "Hampton, VA",
    "email": "ty@lerscott.com",
    "website": "https://ty.lerscott.com",
    "profession": "Senior Software Engineer",
    "linkedin": "linkedin.com/in/tylerscottwilliams"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "1.6.2",
    "@paralleldrive/cuid2": "2.2.2",
    "@sentry/node": "8.17.0",
    "@sentry/profiling-node": "8.17.0",
    "@types/body-parser": "1.19.5",
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/node": "20",
    "@types/object.omit": "3.0.3",
    "body-parser": "1.20.2",
    "clsx": "2.1.1",
    "cors": "2.8.5",
    "dayjs": "1.11.11",
    "deepmerge": "4.3.1",
    "discord-webhook-node": "1.1.8",
    "express": "4.19.2",
    "fdir": "6.1.1",
    "jimp": "0.22.12",
    "object.omit": "3.0.0",
    "puppeteer": "22.12.1",
    "redoc": "2.1.5",
    "tsup": "8.1.0",
    "tsx": "4.16.2",
    "turbo": "^2.0.6",
    "winston": "3.13.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "typescript": "5.5.3"
  }
}
